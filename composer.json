{
    "name": "maimake/largen",
    "description": "laravel generators",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mai",
            "email": "yshxinjian@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "5.7.*",
        "guzzlehttp/guzzle": "^6.3",
        "doctrine/dbal": "^2.8",
        "twig/twig": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Maimake\\Largen\\": "src/"
        },
        "files": ["src/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Maimake\\Largen\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Maimake\\Largen\\LargenServiceProvider"
            ],
            "aliases": []
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    }
}
