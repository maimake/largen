import * as types from './mutation-types'

export const increment = ({commit}) => {
    commit(types.INCREMENT);
};

async function haha() {
    return new Promise((resolve, reject) => {
        setTimeout(_ => {
            resolve();
        }, 1000);
    });
}

export const decrement = async ({commit}) => {

    await haha();
    commit(types.DECREMENT);
};

